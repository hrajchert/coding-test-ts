{
  "name": "coding-test-ts",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "author": "Hernan Rajchert <hrajchert@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsns run",
    "test": "run-s build-and:test",
    "tdd": "run-p watch:*",
    "build": "tsns build",
    "docker:build": "docker build -t ${npm_package_name}:${npm_package_version} .",
    "docker:run": "docker run --rm ${npm_package_name}:${npm_package_version}",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint:* build",
    "prepush": "run-s build-and:test",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-and-fix:-ts": "tslint --project tsconfig.json --fix",
    "build-and:test": "run-s build test-once",
    "build-and:start": "run-s build start",
    "watch:ts": "tsc -w",
    "watch:test": "mocha dist/**/*.test.js -w --colors --inspect --growl",
    "test-once": "mocha dist/**/*.test.js --inspect --colors",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate"
  },
  "dependencies": {
    "@types/sinon": "^7.5.0",
    "enquirer": "^2.3.2",
    "parmenides": "^0.1.0",
    "tsns": "0.0.18",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.7",
    "prettier": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "all-contributors-cli": "^6.4.0",
    "chai": "4.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "sinon": "6.1.5",
    "validate-commit-msg": "2.14.0"
  }
}
